<Window x:Class="BalanceMain.AdminPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BalanceMain"
        mc:Ignorable="d"
        Title="AdminPanel" Height="200" Width="400">
    <Grid>
        <StackPanel VerticalAlignment="Top" Margin="30,10,30,0">
            <Button x:Name="DictionaryName" Content="Изменение словаря" Height="30"  Margin="10" />
            <Button x:Name="ViewDevice" Content="Просмотр устройств" Height="30" Margin="10"  />
        </StackPanel>
    </Grid>
</Window>
