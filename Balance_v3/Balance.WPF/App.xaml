<Application x:Class="Balance.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Balance.WPF"
             xmlns:converter="clr-namespace:Balance.WPF.Converter"
             StartupUri="View/HomeView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <local:ViewModelLocator x:Key="mainViewModelLocator"/>

            <converter:InvertBoolConverter x:Key="invertBoolConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
