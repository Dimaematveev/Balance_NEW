<Application x:Class="Balance.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Balance.WPF"
             xmlns:converter="clr-namespace:Balance.WPF.Converter"
             StartupUri="View/HomeView.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/ButtonCircle.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
            
            <converter:InvertBoolConverter x:Key="invertBoolConverter"/>
            <local:ViewModelLocator x:Key="mainViewModelLocator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
